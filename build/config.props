<Project ToolsVersion="15.0">

  <!-- Dependency versions -->
  <PropertyGroup>
    <NuGetPackageVersion>6.0.0</NuGetPackageVersion>
    <MicrosoftAzureStorageBlobVersion>11.2.3</MicrosoftAzureStorageBlobVersion>
    <JsonVersion>13.0.1</JsonVersion>
    <CommandLineUtilsVersion>2.0.0</CommandLineUtilsVersion>
    <NuGetTestHelpersVersion>2.1.23</NuGetTestHelpersVersion>
    <PortablePdbVersion>1.5.0</PortablePdbVersion>
    <AWSSDKVersion>3.7.4.9</AWSSDKVersion>
    <AWSSDKTokenVersion>3.7.1.92</AWSSDKTokenVersion>
    <DotNetConfigVersion>1.0.6</DotNetConfigVersion>
  </PropertyGroup>

  <!-- Config -->
  <PropertyGroup>
    <RepositoryName>Sleet</RepositoryName>
  </PropertyGroup>

  <!-- Nuspec defaults -->
  <PropertyGroup>
    <PackageProjectUrl>https://github.com/emgarten/Sleet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/emgarten/Sleet</RepositoryUrl>
    <PackageTags>nuget nugetserver nugetfeed sleet</PackageTags>
    <Description>A static nuget feed generator for Azure Storage, AWS S3, and more.</Description>
  </PropertyGroup>

</Project>
